#define _USE_MATH_DEFINES
#include <iostream>
#include <math.h>
#include <cmath>
using namespace std;

int main(){
    setlocale(LC_ALL, "Russian");

    float x1, x2, y1, y2, s, z;
    int banch;

    cout<<"Введите границы 1 интервала (s, z) "<<endl;
    cin>> s >> z; 

    cout<<"Введите границы 2 интервала (x1, y1) "<<endl;
    cin>> x1 >> y1; 

    cout<<"Введите границы 3 интервала (x2, y2) "<<endl;
    cin>> x2 >> y2; 

    if (s<=x1 && z >=y2) {
        cout<<"YES"<<endl;
        banch = 1;
    }
    else {
        cout<<"NO"<<endl;
        banch = 2;
    }

    cout<<"Banch: "<< banch <<endl;
    return 0;
}

